{
    "name": "midnight/block-zend",
    "description": "Bridge between midnight/block and Zend Framework",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Rudolph Gottesheim",
            "email": "r.gottesheim@loot.at"
        }
