        xsi:noNamespaceSchemaLocation="http://schema.phpunit.de/6.5/phpunit.xsd"
        bootstrap="./tests/bootstrap.php">
    <testsuite name="Midnight\Block\Zend">
        <directory>tests</directory>
    </testsuite>
    <filter>
        <whitelist>
